@model IEnumerable<NekoUchi.MVC.Model.SubscribersView>

@{
    ViewData["Title"] = "Pretplaćeni";
}

<h3>Pretplaćeni</h3>

<table class="table">
    <tr>
        <th>
            Email pretplaćenog
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
        </tr>
    }
</table>

